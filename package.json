{
  "name": "coparrot",
  "version": "1.0.1",
  "description": "Your multilingual Git assistant - AI-powered CLI tool for Git operations",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "coparrot": "./dist/bin/index.js",
    "cpt": "./dist/bin/index.js"
  },
  "files": [
    "dist",
    "locales",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx --watch bin/index.ts",
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "git",
    "cli",
    "assistant",
    "i18n",
    "internationalization",
    "multilingual",
    "terminal",
    "ai",
    "commit-message",
    "ai-powered",
    "developer-tools"
  ],
  "author": "ddVital <ddvital0@gmail.com>",
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddVital/coParrot.git"
  },
  "bugs": {
    "url": "https://github.com/ddVital/coParrot/issues"
  },
  "homepage": "https://github.com/ddVital/coParrot#readme",
  "type": "module",
  "i18n": {
    "defaultLanguage": "en",
    "supportedLanguages": [
      "en",
      "pt-BR",
      "es"
    ],
    "localesPath": "./locales"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@google/genai": "^1.33.0",
    "@inquirer/search": "^4.0.1",
    "axios": "^1.13.2",
    "chalk": "^5.3.0",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.2",
    "coparrot": "^1.0.1",
    "figlet": "^1.9.4",
    "gradient-string": "^3.0.0",
    "inquirer": "^12.2.0",
    "log-update": "^7.0.2",
    "marked": "^14.1.3",
    "marked-terminal": "^7.2.1",
    "micromatch": "^4.0.8",
    "openai": "^6.9.1",
    "ora": "^8.1.1"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/micromatch": "^4.0.10",
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
